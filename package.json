{
  "name": "ta-math",
  "version": "0.3.15",
  "description": "Technical analysis library in javascript. Calculates indicators and overlays from ohlcv data",
  "main": "dist/ta-math.js",
  "types": "dist/ta-math.d.ts",
  "module": "dist/ta-math.esm.js",
  "browser": "dist/ta-math.es5.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "test": "node --experimental-modules --no-warnings ./test/test.js",
    "ci": "npm run build && npm run test",
    "cov": "nyc report --reporter=lcov",
    "examples": "http-server -p 1234 -c-1",
    "expected": "python ./test/expected.py"
  },
  "eslintIgnore": [
    "dist",
    "lib"
  ],
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ],
    "include": "**/ta-math**"
  },
  "devDependencies": {
    "@babel/core": "7.8.3",
    "@babel/preset-env": "7.8.3",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "codecov": "3.6.1",
    "nyc": "15.0.0",
    "rollup": "1.29.0",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-typescript2": "^0.25.3",
    "typescript": "^3.7.5",
    "zora": "^3.1.8"
  },
  "keywords": [
    "technical-indicators",
    "technical-analysis",
    "quantitative-finance",
    "correlation-matrix",
    "finance"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/munrocket/ta-math.git"
  },
  "author": "munrocket",
  "license": "MIT"
}
